S_0x14c704090 .scope module, "counter_tb" "counter_tb" 2 3;
 .timescale 0 0;
v0x14c719540_0 .var "tbclk", 0 0;
v0x14c7195f0_0 .net "tbcount", 31 0, v0x14c719480_0;  1 drivers
S_0x14c704200 .scope module, "DUT" "counter" 2 8, 3 1 0, S_0x14c704090;
 .timescale 0 0;
    .port_info 0 /OUTPUT 32 "count";
    .port_info 1 /INPUT 1 "clk";
v0x14c705ba0_0 .net "clk", 0 0, v0x14c719540_0;  1 drivers
v0x14c719480_0 .var/s "count", 31 0;
E_0x14c709090 .event negedge, v0x14c705ba0_0;
E_0x14c708f10 .event posedge, v0x14c705ba0_0;
    .scope S_0x14c704200;
T_0 ;
    %wait E_0x14c708f10;
    %load/vec4 v0x14c719480_0;
    %addi 1, 0, 32;
    %store/vec4 v0x14c719480_0, 0, 32;
    %vpi_call 3 8 "$display", $time {0 0 0};
    %jmp T_0;
    .thread T_0;
    .scope S_0x14c704200;
T_1 ;
    %wait E_0x14c709090;
    %load/vec4 v0x14c719480_0;
    %addi 1, 0, 32;
    %store/vec4 v0x14c719480_0, 0, 32;
    %jmp T_1;
    .thread T_1;
    .scope S_0x14c704200;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x14c719480_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_0x14c704090;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x14c719540_0;
    %inv;
    %store/vec4 v0x14c719540_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x14c704090;
T_4 ;
    %delay 100000, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x14c704090;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14c719540_0, 0, 1;
    %vpi_call 2 16 "$monitor", $time, "\011Count=\011%d", v0x14c7195f0_0 {0 0 0};
    %end;
    .thread T_5;
